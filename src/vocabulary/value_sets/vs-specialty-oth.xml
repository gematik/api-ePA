                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Daten des elektronischen Medikationsplans nach § 334 Absatz 1 Satz 2 Nummer 4"/>
                </extension>
                <code value="emp"/>
                <display value="Elektronischer Medikationsplan"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString value="Daten der elektronischen Notfalldaten nach § 334 Absatz 1 Satz 2 Nummer 5 und 7"/>
                </extension>
                <code value="nfd"/>
                <display value="Notfalldaten"/>
            </concept>
            <concept>
                <extension url="http://hl7.org/fhir/StructureDefinition/valueset-concept-comments">
                    <valueString
                            value="Daten in elektronischen Briefen zwischen den an der Versorgung der Versicherten teilnehmenden Ärzten und Einrichtungen (elektronische Arztbriefe)"/>
                </extension>
                <code value="eab"/>
